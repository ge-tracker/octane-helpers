{
    "name": "ge-tracker/octane-helpers",
    "description": "Helper code for Laravel Octane",
    "homepage": "https://github.com/ge-tracker/octane-helpers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "James Austen",
            "email": "james@ge-tracker.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0",
        "laravel/octane": "^1.2"
    },
    "require-dev": {
        "gtjamesa/php-standards": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.6",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "GeTracker\\OctaneHelpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeTracker\\OctaneHelpers\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "fix": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
